{
  "presets": [
    [
      "@babel/env",
      {
        "modules": "commonjs",
        "useBuiltIns": false,
        "debug": true
      }
    ],
    "@babel/typescript"
  ],
  "plugins": [
    "@babel/proposal-class-properties",
    "@babel/proposal-object-rest-spread",
    "transform-es2015-template-literals",
    [
      "add-header-comment",
      {
        "files": {
          "src/js/Constants.ts": {
            "header": [
              "This file has been generated by Babel. DO NOT EDIT IT DIRECTLY\n",
              "Edit the source file at src/js/Constants.ts"
            ]
          },
          "src/js/index.ts": {
            "header": [
              "This file has been generated by Babel. DO NOT EDIT IT DIRECTLY\n",
              "Edit the source file at src/js/index.ts"
            ]
          },
          "src/js/RmxAudioPlayer.ts": {
            "header": [
              "This file has been generated by Babel. DO NOT EDIT IT DIRECTLY\n",
              "Edit the source file at src/js/RmxAudioPlayer.ts"
            ]
          }
        }
      }
    ]
  ]
}
